<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Delete Users</title>
    <link href="~/lib/bootstrap/css/bootstrap.min.css" rel="stylesheet" />
    <script src="~/lib/jquery/jquery.min.js"></script>
    <script src="~/lib/bootstrap/js/bootstrap.min.js"></script>
</head>
<body>

    @model VisitorRegistration
    <div class="container">
        <form method="post" action="/Admin/DeleteUserPost">
            <div class="offset-sm-2 col-sm-10">
                <p>
                    <h3>Delete Users</h3>
                </p>
            </div>

            <div class="form-group row">
                <label for="Userid" class="control-label col-sm-2">User ID :</label>
                <div class="col-sm-5">
                    <input type="text" placeholder="Enter User ID" id="Userid" name="Userid" class="form-control" />
                </div>
            </div>

        

            <div class="form-group row">
                <div class="offset-sm-2 col-sm-5">
                    <input type="submit" value="Delete Users" class="btn btn-warning" />
                </div>
            </div>

            <b><a href="Users">Back to Users List</a></b>



            @if (ViewData["Message"] != null)
            {
                <div class="form-group row">
                    <div class="offset-sm-2 col-sm-5">
                        <div class="alert alert-@ViewData["MsgType"]">
                            @ViewData["Message"]
                        </div>
                    </div>
                </div>
            }
        </form>
    </div>
</body>
</html>